// 定义自定义组件
[Custom] @Element Card {
    div {
        class: "card";
        h3 { text { "标题" } }
        p { 
            class: "content";
            text { "内容" } 
        }
        p {
            class: "footer";
            text { "页脚" }
        }
    }
}

// 使用引用 - 删除所有 p 元素
@Element Card {
    delete p;
};