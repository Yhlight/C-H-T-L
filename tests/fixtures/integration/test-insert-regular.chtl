[Custom] @Element Container {
    div {
        p { text { "P1" } }
        p { text { "P2" } }
        p { text { "P3" } }
    }
}

body {
    h2 { text { "插入到特定p后面" } }
    @Element Container {
        insert after p[1] {
            h3 { text { "Inserted after P2" } }
        }
    }
    
    h2 { text { "替换特定p" } }
    @Element Container {
        insert replace p[0] {
            h4 { text { "Replaced P1" } }
        }
    }
}