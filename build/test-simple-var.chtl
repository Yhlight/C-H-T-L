[Template] @Var Colors
{
    primary: "#007bff";
    secondary: "#6c757d";
}

[Template] @Var Sizes
{
    small: "12px";
    medium: "16px";
    large: "20px";
}

body
{
    style
    {
        background-color: Colors(primary);
        font-size: Sizes(medium);
    }
    
    div
    {
        style
        {
            // Test parameter override
            color: Colors(primary = "#ff0000");
            padding: Sizes(small);
        }
    }
}